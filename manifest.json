{
  "name": "Bilibili text translate",
  "version": "1.0.0",
  "description": "Translate bilibili page.",
  "manifest_version": 3,
  "author": "u/XilkyTofu u/Prune_Berry u/konokalahola",
  "host_permissions": ["https://*.bilibili.com/*"],
  "content_scripts": [
    {
      "matches": ["https://*.bilibili.com/*"],
      "js": ["dictionary.js", "main.js"]
    }
  ],
  "icons": {
    "16" : "icon16.png",
    "32" : "icon32.png",
    "48" : "icon48.png",
    "64" : "icon64.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": {
      "16" : "icon16.png",
      "32" : "icon32.png",
      "48" : "icon48.png",
      "64" : "icon64.png",
      "128": "icon128.png"
    }
  }
}
